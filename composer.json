{
    "name": "r3h6/powermail_css",
    "type": "typo3-cms-extension",
    "description": "Three different layouts for powermail forms.",
    "keywords": [
        "TYPO3",
        "extension",
        "powermail"
    ],
    "authors": [
        {
            "name": "R3 H6",
            "email": "r3h6@outlook.com",
            "role": "Developer"
        }
    ],
    "license": "GPL-3.0+",
    "require": {
        "typo3/cms-core": "~7.6.0",
        "typo3-ter/powermail": "~2",
    },
    "require-dev": {
        "mikey179/vfsStream": "1.4.*@dev",
        "phpunit/phpunit": "~4.7.0"
    },
    "autoload": {
        "psr-4": {
            "TYPO3\\PowermailCss\\": "Classes"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TYPO3\\PowermailCss\\Tests\\": "Tests",
            "TYPO3\\CMS\\Core\\Tests\\": ".Build/vendor/typo3/cms/typo3/sysext/core/Tests/"
        }
    },
    "replace": {
        "powermail_css": "self.version",
        "typo3-ter/powermail_css": "self.version"
    },
    "config": {
        "vendor-dir": ".Build/vendor",
        "bin-dir": ".Build/bin"
    }
}